        android:layout_height="wrap_content"
        android:hint='например, Генеральный директор'
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="В лице...:" />

    <EditText
        android:id="@+id/kontragent_vlice"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, Генерального директора Петрова Евгения Юрьевича, действующего на основании Устава'
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ФИО руководителя (сокращенно):" />

    <EditText
        android:id="@+id/kontragent_FIOruksokr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, Петров Е.Ю.'
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Юридический адрес:" />

    <EditText
        android:id="@+id/kontragent_URaddr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, 123456, г.Москва, ул.Цветочная, 99, офис 50'
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Фактический адрес:" />

    <EditText
        android:id="@+id/kontragent_FACTaddr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, 123456, г.Москва, ул.Цветочная, 99, офис 50'
        />
        
        
         <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Почтовый адрес:" />
        
        
         <EditText
        android:id="@+id/kontragent_POSTaddr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, 123456, г.Москва, ул.Цветочная, 99, офис 50'
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Телефон:" />
        

    <EditText
        android:id="@+id/kontragent_Phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, +7 (495) 123 45 67'
        />
        
        
        <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Ответственный за исполнение Договора:" />

    <EditText
        android:id="@+id/kontragent_otvetstvennij"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, Иванов Иван Иванович'
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="Мобильный телефон:" />

    <EditText
        android:id="@+id/kontragent_MobPhone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, +7 (964) 123 45 67'
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Email:" />

    <EditText
        android:id="@+id/kontragent_Email"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, test@test.ru'
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Сайт:" />

    <EditText
        android:id="@+id/kontragent_www"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, www.abcdef.ru'
        />
        
        
        
        
        </LinearLayout>
        
        
        
        
        
