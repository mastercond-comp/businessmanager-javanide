<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
     android:id="@+id/LLContainer"
     android:layout_width="1250dp"
     android:layout_height="wrap_content"
     android:orientation="vertical"
     android:gravity="left|center"
     android:background="#55e8f5e9"
     android:padding="10dp">
    
     <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Добавить сделку" 
        android:textSize="20dp"
        android:textStyle="bold" 
        />
        
    
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Название сделки:" 
        android:layout_marginTop="10dp"
        />

    <EditText
        android:id="@+id/sdelka_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например: Поставка запчастей для ООО "Ромашка"'
        />
        
        
        
        
        
     <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="35dp"
       android:orientation="horizontal"
       android:layout_marginLeft="10dp"
       android:layout_marginTop="10dp">
        
        
        
         <Button
          style="@style/ButtonRASKRITIE"
          android:id="@+id/buttonshowkontragent"
          android:text=">" />
        
        
         <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:text="КОНТРАГЕНТ" 
          android:textSize="16dp"
          android:textStyle="bold"  />
          
          
          
         <Button
          style="@style/ButtonPAGEsdelki" 
          android:id="@+id/buttonselectkontragent"
          android:text="Выбрать" />
        
      </LinearLayout>
      
      
      
      
      <LinearLayout
       android:id="@+id/sectionkontragent"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:orientation="vertical"
       android:layout_marginTop="10dp">
      

      <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Полное наименование организации" 
        />
                

    <EditText
        android:id="@+id/kontragent_myfullname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, Общество с ограниченной ответственностью "Эдельвейс"'
        android:inputType="textMultiLine"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Сокращенное наименование организации/ИП:" />

    <EditText
        android:id="@+id/kontragent_mysokrname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, ООО "Эдельвейс"'
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ИНН:" />

    <EditText
        android:id="@+id/kontragent_INN"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, 123456'
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="КПП:" />

    <EditText
        android:id="@+id/kontragent_KPP"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, 123456'
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ОГРН:" />

    <EditText
        android:id="@+id/kontragent_OGRN"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, 123456'
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Наименование банка:" />

    <EditText
        android:id="@+id/kontragent_bankname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, ПАО "Сбербанк"'
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="БИК банка:" />

    <EditText
        android:id="@+id/kontragent_bankbik"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, 123456'
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Корреспондентский счет банка:" />

    <EditText
        android:id="@+id/kontragent_bankks"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, 123456'
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Номер расчетного счета в банке:" />

    <EditText
        android:id="@+id/kontragent_RS"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint='например, 123456'
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Должность руководителя:" />

    <EditText
        android:id="@+id/kontragent_rukdolzhnost"
